{
    "name": "payum/authorize-net-aim",
    "type": "library",
    "description": "The Payum extension. It provides Authorize.Net AIM payment integration.",
    "keywords": ["payment", "authorize.net", "authorizenet"],
    "homepage": "http://payum.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Kotlyar Maksim",
            "email": "kotlyar.maksim@gmail.com"
        },
        {
            "name":     "Payum project",
            "homepage": "http://payum.org/"
        },
        {
            "name":     "Community contributions",
            "homepage": "https://github.com/Payum/AuthorizeNetAim/contributors"
        }
    ],
    "require": {
        "payum/core": "^1.3",
        "authorizenet/authorizenet": "~1.8"
    },
    "require-dev": {
        "payum/core": "^1.3@dev",
        "phpunit/phpunit": "~4.0",
        "symfony/phpunit-bridge": "~2.8|~3.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "Payum\\AuthorizeNet\\Aim": "" }
    },
    "target-dir": "Payum/AuthorizeNet/Aim",
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        }
    }
}
